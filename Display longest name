
class GFG {
    String longest(String names[], int n) {
        int maxsum=0,sum=0;
        int j=0;
        String res="";
       for(int i=0;i<names.length;i++)
       {
           sum=names[i].length();
           if(maxsum<sum)
           {
               maxsum=sum;
               j=i;
           }
       }
        return names[j];
    }
}
